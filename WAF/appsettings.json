{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IpRateLimitPolicies": {
    "IpRules": []
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": true,
    "RealIpHeader": "X-Forwarded-For",
    "ClientIdHeader": "CF-Connecting-IP",
    "HttpStatusCode": 429,
    "IpWhitelist": [ ],
    "EndpointWhitelist": [ ],
    "ClientWhitelist": [ ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "6s",
        "Limit": 3
      }
    ]
  },
  "Urls":"http://0.0.0.0:80/",
  "WhitelistDomain": [ "localhost", "live.uberforever.eu", "test.uberforever.eu", "13.38.94.69" ],
  "AllowedHosts": "*",
  "Configuration": {
    "Ruleset": {
      "Operator": "OrElse",
      "Rules": [
        {
          "MemberName": "Path",
          "Operator": "EndsWith",
          "Inputs": [ ".php" ]
        },
        {
          "MemberName": "UserAgent",
          "Operator": "IsMatch",
          "TargetValue": "^(curl|java|python)"
        }
      ]
    }
  },
  "ReverseProxy": {
    "Routes": [
      {
        "ClusterId": "live",
        "Match": {
          "Hosts": [
            "live.uberforever.eu"
          ]
        }
      },
      {
        "ClusterId": "test",
        "Match": {
          "Hosts": [
            "test.uberforever.eu"
          ]
        }
      },
      {
        "ClusterId": "custom",
        "Match": {
          "Hosts": [
            "13.38.94.69"
          ]
        }
      }
    ],
    "Clusters": {
      "live": {
        "Destinations": {
          "example.com": {
            "Address": "http://localhost:1000/"
          }
        }
      }
    },
    "Clusters": {
      "test": {
        "Destinations": {
          "example.com": {
            "Address": "http://localhost:1000/"
          }
        }
      }
    },
    "Clusters": {
      "custom": {
        "Destinations": {
          "example.com": {
            "Address": "http://localhost:1000/"
          }
        }
      }
    }

  }
}